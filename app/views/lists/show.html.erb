<h1><%= @list.name %></h1>
<h1><%= link_to "ajouter un bookmark", new_list_bookmark_path(@list) %></h1>
<% @list.bookmarks.each do |bookmark|%>

  <h1> <%= bookmark.comment %> </h1>
  <%= link_to "Delete",
    list_bookmark_path(@list, bookmark),
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
  %>

<% end %>
